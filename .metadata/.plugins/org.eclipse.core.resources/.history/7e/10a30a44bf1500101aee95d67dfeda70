package lesson;

public class lesson05_10 {
	public static void main(String[] args) {
		
		int sum = 0;
		
		for (int i = 1; i <= 100; i += 3) {
		//if(i % 3 == 0) {
        if(12% i == 0) {

			System.out.printf("i : %3d, sum : %4d\n", i , sum);
		  //System.out.printf("i : %3d, sum : %4d\n", i , sum);
		}		
		System.out.println("pass :"+sum);
		}
	}
}
