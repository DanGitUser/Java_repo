package lesson01;

public class CharType {
	public static void main(String[] args) {
		char a = 'A';
		//syso 'ctrl + space'
		System.out.println("a : " + a);
		
		int b = a;
		System.out.println("b : " + b);
		
		int c = 66;
		System.out.println("c : " + c);
		
		int d = a+b;
		System.out.println("d : " + d);
	}
}
