package chapter1;

public class StringEx {
	public static void main(String[] args) {
		//alt + up dwn arrows
		//ctrl + alt + up dwn arrows
		//shift + ctrl + arrows
		String name;
		name = "Nameless";
		
		String name2 = "Nameless";
		String name3 = null;
		String name4 ="";
		
		System.out.println("name2="+name2);
		System.out.println("name3="+name3);
		System.out.println("name4="+name4);
	}

}
